    "geo-distance": "^0.2.0",
    "jest": "^29.0.3",
    "mongodb-memory-server": "^8.9.2",
    "mongoose": "^5.9.10",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/chai": "^4.2.11",
    "@types/dotenv-safe": "^8.1.0",
    "@types/lambda-tester": "^3.6.0",
    "@types/mocha": "^7.0.2",
    "@types/mongoose": "^5.7.14",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "cross-env": "^7.0.2",
    "istanbul": "^0.4.5",
    "lambda-tester": "^4.0.1",
    "nyc": "^15.0.1",
