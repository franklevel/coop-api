{
  "name": "cu-takehome",
  "version": "1.0.0",
  "description": "This is a simple service to lookup your IP and return some useful information.",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "local": "rimraf .build && serverless offline",
